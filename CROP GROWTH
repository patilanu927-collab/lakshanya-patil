// Online C compiler to run C program online
#include <stdio.h>

int main() {
   float rain, growth;
      printf("Enter the rainfall in mm\n");
      scanf("%f", &rain);
      if (rain>=550 && rain<=600){
          printf("crop growth is 100%\n");
      }
      else if (rain>=450 && rain<500){
          printf("crop growth is 75%\n");
      }   
      else if (rain>=250 && rain<350){
          printf("crop growth is 50%\n");
      }   
      else{
          printf("crop growth is less than 5%\n");
      }
      return 0;
   }
